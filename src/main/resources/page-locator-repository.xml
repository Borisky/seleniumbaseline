<?xml version="1.0" encoding="UTF-8"?>

<map>
	<!--locator of page map info -->
	<page pageName="com.seleniumwork.page.DHlogin2">
		<!--Locator lists -->
		<locator type="ByXpath" timeOut="3" value=".//*[@id='username']">logname</locator>
		<locator type="ByXpath" timeOut="3" value=".//*[@id='password']">logpass</locator>
		<locator type="ByXpath" timeOut="3" value="html/body/div[2]/div[3]/img">logbtn</locator>
		
	</page>
	<page pageName="com.seleniumwork.page.LoginGPO">
		<locator type="cssSelector" timeOut="3" value="input#pt-login-username-textbox">logname</locator>
		<locator type="cssSelector" timeOut="3" value="input#pt-login-password-field">logpass</locator>
		<locator type="cssSelector" timeOut="3" value="input.formBtnText">LogIn</locator>
	</page>
	<page pageName="com.seleniumwork.page.HPEFSHome">
		<locator type="cssSelector" timeOut="3" value="select#gNQuickLinks">Communitiess</locator>
		<locator type="cssSelector" timeOut="3" value="select#gNQuickLinks>option[text='Phoenix']">PhoenixLink</locator>
	</page>
	<page pageName="com.seleniumwork.page.Phoenix">
		<locator type="cssSelector" timeOut="3" value="select#gNQuickLinks">Communitiess</locator>
		<locator type="cssSelector" timeOut="3" value="select#gNQuickLinks>option[text='Phoenix']">PhoenixLink</locator>
	</page>
</map>
